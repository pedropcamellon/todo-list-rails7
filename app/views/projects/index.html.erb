<p style="color: green"><%= notice %></p>

<h1>Projects</h1>

<div id="projects">
  <p>
    <strong>Number of projects:</strong>
    <%= @projects.count %>
  </p>

  <%= link_to "New project", new_project_path, class: "mb-4 btn btn-primary" %>

  <% @projects.each do |project| %>
    <%= render project %>
  <% end %>
</div>
